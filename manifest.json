{
  "name": "Volleyball Journal Marker",
  "description": "A browser extension to mark moments in Youtube videos.",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": [
    "bookmarks",
    "tabs",
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_title": "My Bookmarks",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "open-extension": {
      "suggested_key": {
        "mac": "MacCtrl+Command+V"
      },
      "description": "Open the extension."
    },
    "submit-marker": {
      "suggested_key": {
        "mac": "MacCtrl+Command+M"
      },
      "description": "Submit a marker."
    }
  }
}